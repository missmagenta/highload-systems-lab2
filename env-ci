# docker-compose

## applications
CLOUD_CONFIG_HOST=cloud-config
EUREKA_HOST=eureka-server
DB_HOST=db
DB_PORT=5432
JVM_OPTS=-Xms64m -Xmx128m

## compose interpolation
MONGO_INITDB_DATABASE=testbase
MONGO_INITDB_ROOT_USERNAME=mongo
MONGO_INITDB_ROOT_PASSWORD=mongo
MONGO_DB_ADMIN_USERNAME=mongo
MONGO_DB_ADMIN_PASSWORD=mongo
DB_CHANGE_LOG_FILE=changelog/test-changelog-root.yaml

# both compose and app
APP_SERVER_PORT=8080
